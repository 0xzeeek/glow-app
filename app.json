{
  "expo": {
    "name": "glow",
    "slug": "glow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "glow",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSUserNotificationsUsageDescription": "Glow uses notifications to alert you about launches, price moves, and rewards in real time.",
        "branch_key": {
          "live": "YOUR_BRANCH_LIVE_KEY"
        }
      },
      "associatedDomains": [
        "applinks:glow.app.link",
        "applinks:glow-alternate.app.link",
        "applinks:glow.app.link",
        "applinks:glow-alternate.app.link"
      ],
      "bundleIdentifier": "com.fiigmnt.glow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "glow.app.link"
            },
            {
              "scheme": "https",
              "host": "glow-alternate.app.link"
            },
            {
              "scheme": "glow"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "glow.app.link"
            },
            {
              "scheme": "https",
              "host": "glow-alternate.app.link"
            },
            {
              "scheme": "glow"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.fiigmnt.glow"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#000000",
          "sounds": []
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "expo-branch"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "zeke-glow",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f0bfb786-a293-422b-ab63-134eb09cea01"
      }
    }
  }
}
